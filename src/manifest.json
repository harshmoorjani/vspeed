{
  "manifest_version": 3,
  "name": "VSpeed",
  "description": "Speed Up (or slow down) any HTML5 video on any website.",
  "version": "1.0.0",
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
